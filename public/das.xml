                    <STAR>
                        <SIZE>15</SIZE>
                        <POINTS>4</POINTS>
                        <HEIGHT>10</HEIGHT>
                        <FGCOLOR>#9DD563</FGCOLOR>
                        <BGCOLOR>transparent</BGCOLOR>
                        <BUMP>yes</BUMP>
                        <LABEL>no</LABEL>
                    </STAR>
                </GLYPH>
            </TYPE>
